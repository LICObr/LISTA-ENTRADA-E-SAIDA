programa {
  funcao inicio() {
    real l1
    real l2
    real l3
    real volume

    escreva("Digite o comprimento da base do prisma: ")
    leia(l1)
    escreva("Digite a largura da base do prisma: ")
    leia(l2)
    escreva("Digite a altura do prisma: ")
    leia(l3)
    volume = l1 * l2 * l3
    escreva("Volume do prisma: ", volume)
  }
}
