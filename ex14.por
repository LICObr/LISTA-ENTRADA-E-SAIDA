programa {
  funcao inicio() {
    real num1
    real num2
    real subtracao
    real resultado

    escreva("Digite o primeiro número: ")
    leia(num1)
    escreva("Digite o segundo número: ")
    leia(num2)
    subtracao = num1 - num2
    resultado = subtracao / 10
    escreva("Décima parte da subtração: ", resultado)
  }
}
