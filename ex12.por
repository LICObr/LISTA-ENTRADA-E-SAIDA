programa {
  funcao inicio() {
    inteiro num1
    inteiro num2
    inteiro num3
    inteiro produto

    escreva("Digite o primeiro número inteiro: ")
    leia(num1)
    escreva("Digite o segundo número inteiro: ")
    leia(num2)
    escreva("Digite o terceiro número inteiro: ")
    leia(num3)
    produto = num1 * num2 * num3
    escreva("Produto: ", produto)
  }
}
