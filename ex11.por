programa {
  funcao inicio() {
    real nota1
    real nota2
    real nota3
    real media

    escreva("Digite a primeira nota: ")
    leia(nota1)
    escreva("Digite a segunda nota: ")
    leia(nota2)
    escreva("Digite a terceira nota: ")
    leia(nota3)
    media = (nota1 + nota2 + nota3) / 3
    escreva("Média das notas: ", media)
  }
}
