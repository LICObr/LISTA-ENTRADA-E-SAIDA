programa {
  funcao inicio() {
    real num1
    real num2
    real num3
    real resultado

    escreva("Digite o primeiro número: ")
    leia(num1)
    escreva("Digite o segundo número: ")
    leia(num2)
    escreva("Digite o terceiro número: ")
    leia(num3)
    resultado = (num1 + num3) * num2
    escreva("Resultado: ", resultado)
  }
}
